fun valid :: "nat \<Rightarrow> nat \<Rightarrow> char \<Rightarrow> string \<Rightarrow> bool" where
  "valid low high letter password = 
    (let count = (count_list password letter) in
    count \<ge> low \<and> count \<le> high)"


