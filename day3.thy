type_synonym TreeMap = "bool list list"

fun bool_to_nat :: "bool \<Rightarrow> nat" where
  "bool_to_nat False = 0" |
  "bool_to_nat True = 1"

fun nat_to_bool :: "nat \<Rightarrow> bool" where
  "nat_to_bool n = (n > 0)"

fun new_tree :: "nat list list \<Rightarrow> bool list list" where
  "new_tree t = (map (map nat_to_bool) t)"

definition input1 :: TreeMap where
