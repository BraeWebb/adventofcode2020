[False, True, False, False, True, False, False, False, True, False, True]
  ]"

fun wrap_index :: "'a list \<Rightarrow> nat \<Rightarrow> 'a" (infix "!~" 200) where
  "wrap_index l i = (l!(i mod (length l)))"

fun has_tree :: "nat \<Rightarrow> nat \<Rightarrow> TreeMap \<Rightarrow> bool" where
  "has_tree x y m = (if y > ((length m) - 1) then False else (m!~y)!~x)"

type_synonym iter_has_tree = "nat \<Rightarrow> TreeMap \<Rightarrow> bool"

fun count_trees :: "iter_has_tree \<Rightarrow> TreeMap \<Rightarrow> nat \<Rightarrow> nat" where
  "count_trees f m 0 = 0" |
  "count_trees f m i = (bool_to_nat (f i m)) + (count_trees f m (i-1))"

fun solve :: "TreeMap \<Rightarrow> iter_has_tree \<Rightarrow> nat" where
  "solve m f = count_trees f m ((length m)-1)"


fun it_has_tree :: iter_has_tree where
  "it_has_tree i m = has_tree (i*3) i m"

value "solve input1 it_has_tree"


